{
  "name": "googlejk",
  "version": "1.2.4",
  "manifest_version": 2,
  "description": "Google JK is a Chrome extension that provide navigation using 'J' and 'K'.",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*",
        "https://www.google.co.jp/*"
      ],
      "js": ["index.js"],
      "css": ["index.css"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
