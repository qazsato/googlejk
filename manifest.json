{
  "name": "googlejk",
  "version": "1.2.0",
  "manifest_version": 2,
  "description": "googlejk",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*",
        "https://www.google.co.jp/*"
      ],
      "js": ["index.js"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
